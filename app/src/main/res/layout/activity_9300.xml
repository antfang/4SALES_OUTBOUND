<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#dddddd"
        android:orientation="vertical">

        <include
            android:id="@+id/includeTitle"
            layout="@layout/update_location_title"></include>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#dddddd"
                    android:orientation="vertical"
                    android:padding="20dp"
                    android:scrollbars="vertical" >

                    <LinearLayout
                        style="@style/SettingRow"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center">

                        <TextView
                            style="@style/TextCommon"
                            android:text="@string/txt_pallet_no" />

                        <EditText
                            android:id="@+id/pallet_no_9300"
                            style="@style/TextEdit"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:imeOptions="actionDone"
                            android:inputType="none"
                            android:textAlignment="center"  />

                        <Button
                            android:id="@+id/search_9300"
                            android:layout_width="56dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="3dp"
                            android:background="@drawable/btn_bottom_confirm_bg"
                            android:text="@string/txt_search"
                            android:textColor="@color/white" />
                    </LinearLayout>
                    <LinearLayout
                        style="@style/SettingRow"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:visibility="gone"
                        android:gravity="center">

                        <TextView
                            style="@style/TextCommon"
                            android:text=""
                            android:id="@+id/confirm_key_9300"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"/>

                    </LinearLayout>

                    <com.bin.david.form.core.SmartTable
                        android:id="@+id/table_1_9300"
                        android:layout_width="match_parent"
                        android:layout_height="228dp" />

                <LinearLayout
                    style="@style/SettingRow"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center">

                    <TextView
                        style="@style/TextCommon"
                        android:text="@string/txt_delivery_no" />

                    <EditText
                        android:id="@+id/delivery_no_9300"
                        style="@style/TextEdit"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:imeOptions="actionNext"
                        android:textAlignment="center" />

                    <Button
                        android:id="@+id/clear_1_9300"
                        style="@style/Button_X"  />

                    <Button
                        android:id="@+id/Action_1_9300"
                        android:layout_width="34dp"
                        android:layout_height="34dp"
                        android:layout_margin="3dp"
                        android:layout_gravity="center"
                        android:background="@drawable/btn_bottom_confirm_bg"
                        android:text="√"
                        android:textColor="@color/white" />
                </LinearLayout>
                <LinearLayout
                    style="@style/SettingRow"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center">

                    <TextView
                        style="@style/TextCommon"
                        android:text="@string/txt_lot_id_small" />

                    <EditText
                        android:id="@+id/lot_id_9300"
                        style="@style/TextEdit"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:imeOptions="actionDone"
                        android:textAlignment="center" />

                    <Button
                        android:id="@+id/clear_2_9300"
                        style="@style/Button_X"  />
                    <Button
                    android:id="@+id/Action_2_9300"
                    android:layout_width="34dp"
                    android:layout_height="34dp"
                    android:layout_margin="3dp"
                    android:layout_gravity="center"
                    android:background="@drawable/btn_bottom_confirm_bg"
                    android:text="√"
                    android:textColor="@color/white" />
                </LinearLayout>
                <LinearLayout
                    style="@style/SettingRow"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:gravity="center">

                    <TextView
                        style="@style/TextCommon"
                        android:text="Carton No.:" />
                    <TextView
                        style="@style/TextCommon"
                        android:text=""
                        android:id="@+id/carton_no_9300"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent" />

                </LinearLayout>
                <LinearLayout
                    style="@style/SettingRow"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center">

                    <TextView
                        style="@style/TextCommon"
                        android:text="@string/txt_lot_id_big" />

                    <EditText
                        android:id="@+id/lot_id2_9300"
                        style="@style/TextEdit"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:imeOptions="actionDone"
                        android:textAlignment="center" />

                    <Button
                        android:id="@+id/clear_3_9300"
                        style="@style/Button_X"  />
                    <Button
                        android:id="@+id/Action_3_9300"
                        android:layout_width="34dp"
                        android:layout_height="34dp"
                        android:layout_margin="3dp"
                        android:layout_gravity="center"
                        android:background="@drawable/btn_bottom_confirm_bg"
                        android:text="√"
                        android:textColor="@color/white" />
                </LinearLayout>
                <LinearLayout
                    style="@style/SettingRow"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center">

                    <TextView
                        style="@style/TextCommon"
                        android:text="@string/txt_cust_part_no" />

                    <EditText
                        android:id="@+id/cust_part_no_9300"
                        style="@style/TextEdit"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:imeOptions="actionDone"
                        android:textAlignment="center" />

                    <Button
                        android:id="@+id/clear_4_9300"
                        style="@style/Button_X"  />
                    <Button
                        android:id="@+id/Action_4_9300"
                        android:layout_width="34dp"
                        android:layout_height="34dp"
                        android:layout_margin="3dp"
                        android:layout_gravity="center"
                        android:background="@drawable/btn_bottom_confirm_bg"
                        android:text="√"
                        android:textColor="@color/white" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:padding="10dp">

                    <Button
                        android:id="@+id/end_9300"
                        android:layout_width="144dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@drawable/btn_bottom_confirm_bg"
                        android:text="@string/txt_end"
                        android:textColor="@color/white" />

                    <Button
                        android:id="@+id/buttonPrint9300"
                        android:layout_width="160dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginLeft="50dp"
                        android:background="@drawable/btn_bottom_confirm_bg"
                        android:text="打印"
                        android:textColor="@color/white" />

                </LinearLayout>
<!--                <LinearLayout-->
<!--                    android:id="@+id/buttonPrint9300layout"-->
<!--                    android:layout_width="match_parent"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:visibility="visible"-->
<!--                    android:gravity="center"-->
<!--                    android:padding="10dp">-->

<!--                    -->

<!--                </LinearLayout>-->
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>